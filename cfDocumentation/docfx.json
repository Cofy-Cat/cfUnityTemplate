{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": "../Assets/Modules/cfEngine",
          "files": [
            "**/*.cs"
          ],
          "exclude": [
            "**/Test/**",
            "**/Editor/**"
          ]
        },
        {
          "src": "../Assets/Modules/cfUnityEngine",
          "files": [
            "**/*.cs"
          ],
          "exclude": [
            "**/Test/**",
            "**/Editor/**"
          ]
        }
      ],
      "dest": "api",
      "filter": "filterConfig.yml",
      "references": [
        {
          "src": "../Assets/Modules/cfEngine",
          "files": [
            "**/*.dll"
          ]
        },
        {
          "src": "../Library/PackageCache",
          "files": [
            "**/*.dll"
          ],
          "exclude": [
            "**/JIT/**",
            "**/Assemblies/**",
            "**/ARM64/**",
            "**/x64/**",
            "**/x86/**",
            "**/com.unity.collab-proxy/**"
          ]
        },
        {
          "src": "../Library/ScriptAssemblies",
          "files": [
            "**/*.dll"
          ],
          "exclude": [
            "cfEngine.dll",
            "cfUnityEngine.dll"
          ]
        },
        {
          "src": "unity_lib",
          "files": [
            "**/*.dll"
          ]
        }
      ]
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Cofy Dev Unity Template Documentation",
      "_appTitle": "Cofy Dev Unity Template Documentation",
      "_enableSearch": true,
      "pdf": true
    }
  }
}